local Mini = function()
    if r12_1.Text == "-" then
        r12_1.Text = "+"
        r2_1.Visible = false
    end
    if r12_1.Text == "+" then
        r12_1.Text = "-"
        r2_1.Visible = true
    end
end

r12_1.MouseButton1Click:Connect(Mini)

