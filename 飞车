r5_1.MouseButton1Click:Connect(function()
    r5_1.Visible = false
    r5_1.Text = "开"
    r5_1.TextColor3 = Color3.fromRGB(0, 255, 0)
    r9_1.Visible = true
    r13_1.Visible = true
    local BodyVelocity = Instance.new("BodyVelocity", game.Players.LocalPlayer.Character.HumanoidRootPart)
    local BodyGyro = Instance.new("BodyGyro", game.Players.LocalPlayer.Character.HumanoidRootPart)
    BodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
    game:GetService("RunService").RenderStepped:Connect(function()
        BodyGyro.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
        BodyGyro.D = 5000
        BodyGyro.P = 100000
        BodyGyro.CFrame = game.Workspace.CurrentCamera.CFrame
    end)
end)

